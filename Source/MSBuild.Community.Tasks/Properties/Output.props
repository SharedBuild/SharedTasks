<?xml version="1.0" encoding="UTF-8"?>
<Project>
	<PropertyGroup>
		<IntermediateOutputPath>obj\$(Configuration)\$(TargetFramework)\$(TargetPlatform)\</IntermediateOutputPath>
		<OutputPath>bin\$(Configuration)\$(TargetFramework)\$(TargetPlatform)\</OutputPath>
		<DocumentationFile Condition="'$(DocumentationFile)' == ''">$(OutputPath)</DocumentationFile>
		<PackageOutputPath Condition="'$(PackageOutputPath)' == ''">$(ArtifactOutputPath)</PackageOutputPath>
	</PropertyGroup>
</Project>